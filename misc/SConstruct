gcc_command = "$CC -o $TARGET $CCFLAGS $SOURCES"
strip_command = "strip -s -o $TARGET $SOURCES"

Command("empty_main", "empty_main.c", gcc_command)
Command("empty_main_pic", "empty_main.c", gcc_command, CCFLAGS="-fPIC")
Command("empty_main_gdb", "empty_main.c", gcc_command, CCFLAGS="-ggdb")
Command("init_array", "init_array.c", gcc_command)
Command("hook", "hook.c", gcc_command)
Command("hook_pic", "hook.c", gcc_command, CCFLAGS="-fPIC")
Command("hook_pic_x86", "hook.c", gcc_command, CCFLAGS="-m32 -fPIC")
Command("hook_x86", "hook.c", gcc_command, CCFLAGS="-m32")
Command("start_nostdlib", "start_nostdlib.c", gcc_command, CCFLAGS="-nostdlib")
Command("start_nostdlib_strip", "start_nostdlib", strip_command)
Command("scan_x86", "scan.c", gcc_command, CCFLAGS="-m32")
Command("scan", "scan.c", gcc_command)
Command("strcpy_crash_x86", "strcpy_crash.c", gcc_command, CCFLAGS="-m32")
Command("strcpy_crash", "strcpy_crash.c", gcc_command)
Command("callee_params", "callee_params.c", gcc_command)
Command("callee_params_x86", "callee_params.c", gcc_command, CCFLAGS="-m32")
Command("ret_val_x86", "ret_val.c", gcc_command, CCFLAGS="-m32")
Command("ret_val", "ret_val.c", gcc_command)
Command("goto1", "goto1.c", gcc_command)
Command("goto1_x86", "goto1.c", gcc_command, CCFLAGS="-m32")
Command("condjmp", "condjmp.c", gcc_command)
Command("condjmp_x86", "condjmp.c", gcc_command, CCFLAGS="-m32")
Command("loop1", "loop1.c", gcc_command)
Command("loop1_x86", "loop1.c", gcc_command, CCFLAGS="-m32")
Command("memcp", "memcp.c", gcc_command)
Command("memcp_x86", "memcp.c", gcc_command, CCFLAGS="-m32")
Command("switch1", "switch1.c", gcc_command)
Command("switch1_x86", "switch1.c", gcc_command, CCFLAGS="-m32")
Command("switch2", "switch2.c", gcc_command)
Command("switch2_x86", "switch2.c", gcc_command, CCFLAGS="-m32")
Command("switch3", "switch3.c", gcc_command)
Command("switch3_x86", "switch3.c", gcc_command, CCFLAGS="-m32")
Command("is_whitespace", "is_whitespace.c", gcc_command)
